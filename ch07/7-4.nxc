task main()
{
   PlayTone(5000,30);
   OnFwdSync(OUT_AC,50,0);//直走
   Wait(1000);
   PlayTone(5000,20);
   OnFwdSync(OUT_AC,50,20);//走弧線
   Wait(1000);
   PlayTone(5000,30);
   OnFwdSync(OUT_AC,50,-50);//轉彎
   Wait(1000);
   PlayTone(5000,30);
   OnRevSync(OUT_AC,50,100); //原地旋轉
   Wait(1000);
   Off(OUT_AC);
}

